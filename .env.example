# Local development example env file

# Preferred variable name used by Payload config
DATABASE_URI=mongodb://127.0.0.1/your-database-name

# Alternative commonly used name (also supported by config)
MONGODB_URI=

PAYLOAD_SECRET=YOUR_SECRET_HERE
PAYLOAD_PUBLIC_SERVER_URL=http://localhost:3000

# Next.js frontend -> Payload GraphQL endpoint
NEXT_PUBLIC_PAYLOAD_GRAPHQL_URL=http://localhost:3000/api/graphql

